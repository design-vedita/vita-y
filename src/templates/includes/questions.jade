        .quest-form
            .container
                form.universal
                    fieldset
                        legend  Задать свой вопрос
                        .row
                            .col-md-3.col-sm-6
                                label   Ваше имя
                                input(type="text")
                            .col-md-3.col-sm-6
                                label   E-mail
                                input(type="email")
                        .row
                            .col-md-12
                                label.textarea   Сообщение
                                textarea
                        .submit-block
                            .row
                                .col-md-2.col-sm-2
                                    .captcha
                                        |   Сюда вставить капчу
                                .col-md-3.col-sm-3
                                    input(type="submit" value="Отправить")
